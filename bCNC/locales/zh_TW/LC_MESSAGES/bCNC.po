msgid "# Blocks:"
msgstr "# Blocks:"

#: bCNC/TerminalPage.py:182
msgid "$ Display build information of Grbl"
msgstr "$ 顯示 Grbl 的建譯資訊"

#: bCNC/TerminalPage.py:103
msgid "$# Display parameters of Grbl"
msgstr "$# 顯示Grbl的參數"

#: bCNC/TerminalPage.py:88
msgid "$$ Display settings of Grbl"
msgstr "$$ Grbl 的顯示設置"

